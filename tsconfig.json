{
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "lib": ["esnext","dom"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "types": [
      "vite/client",
      "unplugin-vue-router/client"
    ]
  }
}
